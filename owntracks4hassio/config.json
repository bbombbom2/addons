{
  "name": "Owntracks Recorder",
  "url": "https://git.asperti.com/paspo/hassio-addons",
  "image": "docker.asperti.com/paspo/hassio-ot-recorder",
  "version": "0.2.2",
  "slug": "ot-recorder",
  "description": "Owntracks Recorder - connects to your mqtt broker and consumes owntracks locations",
  "startup": "application",
  "boot": "auto",
  "ports": {
    "8083": 8083
  },
  "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "i386"
  ],
  "webui": "http://[HOST]:[PORT:8083]",
  "map": ["share:rw"],
  "ingress": true,
  "ingress_entry": "/",
  "ingress_port": 8083,
  "panel_icon": "mdi:map-clock",
  "panel_title": "OwntracksREC",
  "options": {
    "topics":	"owntracks/#",
    "host":	"IPADDRESS_OR_HOSTNAME",
    "port":	1883,
    "user":	"DVES_USER",
    "pass":	"DVES_PASS",
    "qos":	2
  },
  "schema": {
    "topics":	"str",
    "host":	"str",
    "port":	"port",
    "user":	"str",
    "pass":	"str",
    "qos":	"int"
  }
}

